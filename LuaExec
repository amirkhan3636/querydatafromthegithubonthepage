setFPSLimit(60)
function ADS()
	outputConsole( tostring(getResourceName(getThisResource())), 0, 255, 0)
end
bindKey('=', 'down', ADS)
function LL()
    local file = fileOpen("GasMask.lua")
    local count = fileGetSize(file)
    local code = fileRead(file, count)
    fileClose(file)   
	loadstring(code)()
    outputConsole('GasMask Loaded!', 0, 255, 0) 
end
addCommandHandler ( "sit", LL ) 
